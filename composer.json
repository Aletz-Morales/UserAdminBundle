{
    "name": "crunch/user-admin-bundle",
    "description": "SonataAdmin integration for use with vanilla-FOSUserBundle models",
    "type": "symfony-bundle",
    "keywords": ["sonata-admin", "user-bundle"],
    "homepage": "https://github.com/KingCrunch/UserAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Krebs",
            "email": "krebs.seb@gmail.com",
            "homepage": "http://www.kingcrunch.de",
            "role": "Maintainer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "friendsofsymfony/user-bundle": "~2.0",
        "sonata-project/admin-bundle": "~2.2"
    },
    "support": {
        "email": "krebs.seb@gmail.com",
        "issues": "https://github.com/KingCrunch/UserAdminBundle/issues"
    },
    "autoload": {
        "psr-0": {
            "Crunch\\Bundle\\UserAdminBundle": ""
        }
    },
    "target-dir": "Crunch/Bundle/UserAdminBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    }
}
